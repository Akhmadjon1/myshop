<!doctype html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="/main.css">
    <script src="//maxcdn.bootstrapcdn.com/bootstrap/4.1.1/js/bootstrap.min.js"></script>
</head>

<body>
    <div class="frame">
        <div class="main_frame"> 
            <div class="left_frame">
                <div class="profil">
                    <div class="picture"><img src="https://i.ibb.co/Gp62brj/my-passport-photo-removebg-preview.png" width="156px" height="142px" alt=""></div>
                    <div class="name"><%=user.name%></div>
                    <div class="profession"><%=user.profession%></div>
                    <div class="icons">
                    <img src="icon/facebook.svg" alt="" class="">
                    <img src="icon/instagram.svg" alt="" class=""> 
                    <img src="icon/twitter.svg" alt="" class=""> 
                    <img src="icon/youtube.svg" alt="" class="">
                    <img src="icon/linkeding.svg" alt="" class="">
                    <div class="dribbble"> <img src="icon/vector.svg" alt="" class="drib-pic"></div>
                    </div>
                </div>
                    <div class="line"></div>
                <div class="description">
                    <div class="age">
                        <div class="age1">Age:</div>
                        <div class="age2"><%=user.age%></div>
                     </div>
                     <div class="residence">
                        <div class="residence1">Residence:</div>
                        <div class="residence2"><%=user.residence%></div>
                    </div>
                    <div class="freelance">
                        <div class="freelance1">Freelance:</div>
                        <div class="freelance2"><%=user.freelance%></div>
                    </div>
                    <div class="adress">
                        <div class="adress1">Adress:</div>
                        <div class="adress2"><%=user.adress%></div>
                    </div>
               </div>
               <div class="line"></div>
               <div class="languages">
                   <div class="languages1">Languages</div>
                    <div class="uzbek">
                        <div class="uzbek1">Uzbek</div>
                        <div class="uzbek2">100%</div>
                    </div>
                    <div class="rectangle"><div class="rectangle1"></div> </div>
                    <div class="english">
                        <div class="english1">English</div>
                        <div class="english2">85%</div>
                    </div>
                    <div class="rectangle2"><div class="rectangle3"></div> </div> 
                    <div class="korean">
                        <div class="korean1">Korean</div>
                         <div class="korean2">50%</div>
                    </div>
                    <div class="rectangle4"><div class="rectangle5"></div> </div>
                </div>
                
                <div class="line"></div>

                <div class="skills">
                    <div class="skills1">Skills</div>
                    <div class="html">
                        <div class="html1">HTML</div>
                        <div class="html2">90%</div>
                    </div>
                    <div class="rectangle6"><div class="rectangle7"></div> </div>
                    <div class="css">
                        <div class="css1">CSS</div>
                        <div class="css2">85%</div>
                    </div>
                        <div class="rectangle8"><div class="rectangle9"></div> </div>
                    <div class="js">
                        <div class="js1">Js</div>
                        <div class="js2">80%</div>
                    </div>
                    <div class="rectangle10"><div class="rectangle11"></div> </div>
                    <div class="php">
                        <div class="php1">PHP</div>
                        <div class="php2">75%</div>
                     </div>
                        <div class="rectangle12"><div class="rectangle13"></div> </div>    
                        <div class="wordpress">
                            <div class="wordpress1">WordPress</div>
                            <div class="wordpress2">85%</div>
                        </div>
                            <div class="rectangle14"><div class="rectangle15"></div> </div>  
                </div>
             
                <div class="line"></div>

                <div class="extra_skills">
                    <div class="extra_skills1">Extra Skills</div>
                <div class="double_icons"><img src="icon/icons.svg" alt="">
                    <div class="skill_name">Bootstrap, Materialize</div>
                </div>
    
                <div class="double_icons1"><img src="icon/icons.svg" alt="">
                    <div class="skill_name1">Stylus, Sass, Less</div>
                </div>

                <div class="double_icons2"><img src="icon/icons.svg" alt="">
                    <div class="skill_name2"> Gulp, Webpack, Grunt</div>
                </div>
                <div class="double_icons3"><img src="icon/icons.svg" alt="">
                    <div class="skill_name3">GIT Knowledge</div>
                </div>

                </div>



                <div class="line"></div>
            </div>
            <div class="midd_frame">
                <div class="midd_top">2.1</div>
                <div class="midd_bot">2.2</div>

            </div>
            <div class="right_frame">3</div>
        </div> 
    </div>

</body>

</html>